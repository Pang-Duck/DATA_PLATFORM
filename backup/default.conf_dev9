    #server_name 211.115.206.9;
    #return 301 https://www.ctilab.cloud;
#}
server {
    #listen 443 ssl default_server;
    #listen [::]:443 ssl ctilab.cloud;
    listen  443 ssl;
    server_name www.ctilab.cloud;
    #ssl_certificate /data/openssl/server_key.cert; 
    #ssl_certificate_key /data/openssl/server_key.pem;

    ssl_certificate /data/ssl/www.ctilab.cloud_20230711355D1.crt;
    ssl_certificate_key /data/ssl/www.ctilab.cloud_20230711355D1.key;
    ssl_prefer_server_ciphers on;
    root /data/www;
    index index.html index.htm index.nginx-debian.html;

        location /#/Login {
        root /data/www;
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        }

    location /v1 {
                proxy_pass http://www.ctilab.cloud:3000/v1/api-docs;
                proxy_set_header Host $host;
        }

    location /api {
                proxy_pass https://www.ctilab.cloud:3001/api;
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        }

    location /socket.io {
                proxy_pass https://www.ctilab.cloud:3001/socket.io;
        proxy_http_version 1.1;
        proxy_set_header Host $host;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection $connection_upgrade;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    }